<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <%- include('../../templates/global.ejs') %>
    <title>Interview - Student Panel</title>
    <style>
      .mytable td,
      th {
        padding: 0.5rem 1rem;
      }

      .hidden-imp {
        display: none !important;
      }
    </style>
  </head>
  <body class="bg-gradient-to-br from-green-300 to-blue-300 min-h-screen">
    <%- include('../../templates/header-7-by-8.ejs') %>

    <div class="flex items-center content-center align-middle">
      <%- include('../../templates/student-sidemenu.ejs') %>
      <section class="p-10 w-6/8 md:w-7/8 mx-auto mr-0" id="main-section">
        <div
          id="assessments-box"
          class="bg-white w-7/8 rounded-lg m-auto mt-16"
        >
          <div
            id="assessments-title-box"
            class="bg-iec-blue text-white font-light px-4 py-3 rounded-t-lg"
          >
            My Interviews
          </div>
          <div id="assessments-box-content" class="px-10 py-8 overflow-x-auto">
            <p>Sorry, you have no interviews yet.</p>
          </div>
        </div>
      </section>
    </div>
  </body>
</html>
