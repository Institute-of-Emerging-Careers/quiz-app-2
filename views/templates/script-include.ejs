<% 
    const script_type = process.env.NODE_ENV === 'production' ? 'text/javascript' : 'text/babel';
    const script_url = `${process.env.NODE_ENV === 'production' ? '/precompiled-' : '/'}js/${script_file_name}`
%>
<script 
    type="<%=script_type%>" 
    src="<%=script_url%>" 
    <% if (typeof defer !== 'undefined' && defer) {%> defer <% } %>
></script>