{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard",
        "eslint:recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-undef":"error",
        "indent": ["error", "tab"],
        "camelcase":"off",
        "no-tabs": "off",
        "comma-dangle": "off",
        "curly": "off",
        "array-callback-return": "warn",
        "no-async-promise-executor": "warn",
        "no-multiple-empty-lines": "error"
    }
}
